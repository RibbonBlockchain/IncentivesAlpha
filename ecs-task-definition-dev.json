{
    "family": "backend-dev",
    "executionRoleArn": "arn:aws:iam::280938191785:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "backend-dev-c",
            "image": "280938191785.dkr.ecr.eu-central-1.amazonaws.com/backend-dev",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "options": {
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "ecs",
                    "awslogs-group": "/ecs/backend-dev"
                },
                "logDriver": "awslogs"
            },
            "environment": [
                {
                    "name": "MONGO_USERNAME",
                    "value": "admin"
                },
                {
                    "name": "MONGO_PASSWORD",
                    "value": "ribbon2021"
                },
                {
                    "name": "MONGO_HOSTNAME",
                    "value": "ec2-3-125-57-245.eu-central-1.compute.amazonaws.com"
                },
                {
                    "name": "MONGO_PORT",
                    "value": "27111"
                },
                {
                    "name": "MONGO_DB",
                    "value": "ribbon_dev"
                },
                {
                    "name": "MONGO_AUTHSOURCE",
                    "value": "admin"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048"
}